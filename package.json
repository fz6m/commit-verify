{
  "name": "commit-verify",
  "version": "1.0.3",
  "bin": {
    "cv": "bin/cv.js"
  },
  "description": "Auto commit message format verify hook",
  "keywords": [
    "git-commit-message",
    "git-commit",
    "commit-lint",
    "commit-verify",
    "commit-emoji"
  ],
  "homepage": "https://github.com/fz6m/commit-verify#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/fz6m/commit-verify"
  },
  "license": "MIT",
  "author": "fz6m",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist && tsc -p ./tsconfig.build.json",
    "test:dev": "node ./bin/cv.js",
    "test": "jest",
    "prepare": "husky install",
    "check": "node ./bin/cv.js",
    "prepublishOnly": "pnpm build",
    "push": "npm_config_registry=https://registry.npmjs.com/ npm publish"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cosmiconfig": "^7.1.0",
    "emoji-regex": "^10.2.1",
    "lodash": "^4.17.21",
    "v8-compile-cache": "^2.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/yargs": "^17.0.24",
    "esbuild": "^0.18.6",
    "esbuild-jest": "^0.5.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "packageManager": "pnpm@8.6.0"
}
