{
  "name": "commit-verify",
  "version": "1.0.2",
  "bin": {
    "cv": "bin/cv.js"
  },
  "description": "Auto commit message format verify hook",
  "keywords": [
    "git-commit-message",
    "git-commit",
    "commit-lint",
    "commit-verify",
    "commit-emoji"
  ],
  "homepage": "https://github.com/fz6m/commit-verify#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/fz6m/commit-verify"
  },
  "license": "MIT",
  "author": "fz6m",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsc --watch",
    "build": "rimraf dist && tsc -p ./tsconfig.build.json",
    "test:dev": "node ./bin/cv.js",
    "test": "jest",
    "prepare": "husky install",
    "check": "node ./bin/cv.js",
    "prepublishOnly": "pnpm build",
    "push": "npm_config_registry=https://registry.npmjs.com/ npm publish"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cosmiconfig": "^7.0.1",
    "emoji-regex": "^10.0.0",
    "lodash": "^4.17.21",
    "swno": "^1.1.1",
    "v8-compile-cache": "^2.3.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/yargs": "^17.0.8",
    "esbuild": "^0.14.23",
    "esbuild-jest": "^0.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "only-allow": "^1.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
